  "homepage": "https://github.com/sieteunoseis/mcp-cisco-support#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
