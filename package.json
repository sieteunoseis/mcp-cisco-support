  "version": "1.0.1",
  "description": "MCP server for Cisco Support APIs including Bug Search and future tools",
  "main": "dist/index.js",
  "bin": {
    "mcp-cisco-support": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CLAUDE.md",
